{
  "manifest_version": 3,
  "name": "Ancestry Tree Exporter",
  "version": "1.0",
  "description": "Export your Ancestry tree data",
  "permissions": [
    "activeTab",
    "downloads",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.ancestry.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
