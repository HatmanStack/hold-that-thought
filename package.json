{
  "name": "urara",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.10.0",
  "author": "Ëóç+85CD",
  "license": "WTFPL",
  "homepage": "https://github.com/importantimport/urara",
  "repository": "importantimport/urara",
  "bugs": "https://github.com/importantimport/urara/issues",
  "scripts": {
    "build": "run-s _tsc _urara:build _kit:build clean",
    "check": "run-s check:types check:lint check:test",
    "check:types": "svelte-check --tsconfig ./tsconfig.json",
    "check:lint": "eslint --flag unstable_ts_config .",
    "check:test": "vitest run",
    "clean": "node urara.js clean",
    "deploy": "bash backend/scripts/deploy.sh",
    "dev": "run-s _tsc \"_dev:parallel {@} \" --",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:load": "artillery run tests/load/comments-load.yml && artillery run tests/load/messages-load.yml && artillery run tests/load/profile-load.yml",
    "_dev:parallel": "run-p -r _tsc:watch _urara:watch \"_kit:dev {@} \" --",
    "_kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "_kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite dev",
    "_tsc": "tsc -p tsconfig.node.json",
    "_tsc:watch": "tsc -w -p tsconfig.node.json",
    "_urara:build": "node urara.js build",
    "_urara:watch": "node urara.js watch"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.782.0",
    "@aws-sdk/client-lambda": "^3.782.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "aws-sdk": "^2.1692.0",
    "gray-matter": "^4.0.3",
    "jose": "^6.0.12",
    "marked": "^15.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.1",
    "@aws-sdk/client-dynamodb": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@iconify-json/heroicons-outline": "^1.1.10",
    "@iconify-json/heroicons-solid": "^1.1.11",
    "@importantimport/eslint-config": "1.0.0-alpha.2",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-netlify": "^4.1.0",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/adapter-vercel": "^5.1.0",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.17",
    "@types/unist": "^3.0.2",
    "@unocss/eslint-plugin": "^0.62.3",
    "@unocss/extractor-svelte": "^0.55.7",
    "@vite-pwa/sveltekit": "^1.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "artillery": "^2.0.26",
    "aws-sdk-client-mock": "^4.1.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "daisyui": "^4.6.2",
    "eslint": "^9.10.0",
    "eslint-plugin-svelte": "^2.43.0",
    "fenceparser": "^2.2.0",
    "fff-flavored-frontmatter": "1.0.0-alpha.1",
    "github-slugger": "^2.0.0",
    "jiti": "^2.6.1",
    "mdast-util-to-string": "^3.2.0",
    "mdsvex": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-lightningcss": "^0.7.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-fff": "1.0.0-alpha.1",
    "remark-footnotes": "~2.0.0",
    "sanitize-html": "^2.17.0",
    "shiki-twoslash": "^3.1.2",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.4",
    "svelte-eslint-parser": "^0.41.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-embed": "^0.0.14",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unist-util-visit": "^4.1.2",
    "unocss": "^0.58.5",
    "uuid": "^13.0.0",
    "vite": "^5.1.1",
    "vite-imagetools": "^4.0.19",
    "vite-plugin-pwa": "^0.17.5",
